<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huaweicloud.servicestage.demo.mapper.UserMapper">

  <select id="selectUserById" resultType="com.huaweicloud.servicestage.demo.entity.UserInfo">
    select id id, userName userName, realName realName from userInfo where id = #{id}
  </select>

</mapper>